                                    OnSelect: |+
                                      =Set(
                                          varStartDateTime,
                                          DateValue(CEStartDateInput_2.SelectedDate) + 
                                          TimeValue(
                                              Dropdown_StartHours.Selected.Value & ":" & 
                                              Dropdown_StartMinutes.Selected.Value
                                          )
                                      ); // combine start date and time

                                      Set(
                                          varEndDateTime,
                                          DateValue(CEEndDateInput_2.SelectedDate) + 
                                          TimeValue(
                                              Dropdown_EndHours.Selected.Value & ":" & 
                                              Dropdown_EndMinutes.Selected.Value
                                          )
                                      ); // combine end date and time

                                      Collect(
                                          EventTests,
                                          {
                                              EventName: CETitleTextInput_4.Value,
                                              EventDetail: CEBodyTextInput_2.Value,
                                              EndDate: varEndDateTime,
                                              StartDate: varStartDateTime,
                                              TagsTest: Concat(CETagInput_2.SelectedItems, TagName, ","), 
                                              Color: CELegendColorInput_2.Selected.LegendColorName
                                          }
                                      ); // collect events

                                      // reset
                                      Reset(CETagInput_2);
                                      Reset(CEStartDateInput_2);
                                      Reset(CEEndDateInput_2);
                                      Reset(CETitleTextInput_4);
                                      Reset(CEBodyTextInput_2);
                                      Reset(CELegendColorInput_2);
                                      Reset(Dropdown_StartHours);
                                      Reset(Dropdown_StartMinutes);
                                      Reset(Dropdown_EndHours);
                                      Reset(Dropdown_EndMinutes);

                                      Set(showcreateevent, false);
                                    Text: ="Save"
