                                  Control: Button@0.0.44
                                  Properties:
                                    BasePaletteColor: =RGBA(249, 83, 109, 1)
                                    Font: =Font.Lato
                                    FontSize: =13
                                    Height: =30
                                    OnSelect: |+
                                      =Set(showDeleteAnnouncement, false); // Hide "Delete Announcement?" Popup

                                      // Store only the AnnouncementID before deletion
                                      Set(deletedAnnouncementID, Gallery19_5.Selected.AnnouncementID);

                                      // Remove the selected announcement
                                      Remove(Announcements, Gallery19_5.Selected);

                                      // Refresh the data source
                                      Refresh(Announcements);

                                      Set(varRegistrarViewContainer, false);
                                      Set(varRegistrarAncView, false);

                                      // Verify that the announcement is gone before showing confirmation
                                      If(
                                          IsBlank(LookUp(Announcements, AnnouncementID = deletedAnnouncementID)), 
                                          Set(showDAConfirmPopup, true) // Show "Deleted successfully" popup   
                                      );


                                    Text: ="Delete"
                                    Width: =90
